<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="40.0" spacing="15.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <opaqueInsets>
      <Insets/>
   </opaqueInsets>
   <padding>
      <Insets left="20.0" right="20.0"/>
   </padding>
   <Text fx:id="nameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
      <HBox.margin>
         <Insets/>
      </HBox.margin>
   </Text>
   <Text fx:id="correctText" strokeType="OUTSIDE" strokeWidth="0.0" text="Correct: 0"/>
   <Text fx:id="wrongText" strokeType="OUTSIDE" strokeWidth="0.0" text="Wrong 0"/>
   <Button fx:id="deleteButton" mnemonicParsing="false" text="delete"/>
   <Button fx:id="continueButton" mnemonicParsing="false" text="continue"/>
</HBox>
