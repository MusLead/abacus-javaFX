<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   
      <Text fx:id="StatusText" strokeType="OUTSIDE" strokeWidth="0.0" text="WRONG!" />
      <HBox fx:id="answerContainer" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Your Answer:" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="2 + 2 = 5" />
               
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Right Anwer:" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="2 + 2 = 4" />
               
            </VBox>
         
      </HBox>
      <Accordion fx:id="accordionSetting">
        <panes>
           <TitledPane animated="false" text="Setting">
              <VBox prefHeight="200.0" prefWidth="100.0">

                 <HBox alignment="BOTTOM_LEFT" prefHeight="43.0" prefWidth="600.0">

                    <VBox alignment="BOTTOM_CENTER" prefHeight="43.0" prefWidth="170.0">

                       <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Operation"/>

                    </VBox>
                    <VBox alignment="BOTTOM_CENTER" prefHeight="43.0" prefWidth="431.0">

                       <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Range of Number"/>

                    </VBox>

                 </HBox>
                 <HBox alignment="CENTER" prefHeight="89.0" prefWidth="600.0" spacing="40.0">

                    <MenuButton fx:id="optMenuButton" mnemonicParsing="false" text="opt">
                       <items>
                          <MenuItem mnemonicParsing="false" text="+"/>
                          <MenuItem mnemonicParsing="false" text="-"/>
                          <MenuItem mnemonicParsing="false" text="*"/>
                          <MenuItem mnemonicParsing="false" text="/"/>
                       </items>
                    </MenuButton>
                    <TextField fx:id="originField"/>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="to"/>
                    <TextField fx:id="boundField"/>

                    <padding>
                       <Insets left="20.0" right="20.0"/>
                    </padding>
                 </HBox>

              </VBox>
           </TitledPane>
        </panes>
      </Accordion>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100.0">
         
            <Button fx:id="mainMenuButton" mnemonicParsing="false" text="Back to Main Menu" />
            <Button fx:id="overViewButton" mnemonicParsing="false" text="Back to Overview" />
            <Button fx:id="continueButton" mnemonicParsing="false" text="continue" />
         
      </HBox>
   
</VBox>
